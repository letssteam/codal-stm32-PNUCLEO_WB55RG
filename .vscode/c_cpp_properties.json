{
    "configurations": [
        {
            "includePath": [
                "/usr/lib/arm-none-eabi/include/**",
                "${workspaceFolder}/inc/**",
                "${workspaceFolder}/src/**",
                "${workspaceFolder}/model/**",
                "${workspaceFolder}/samples/**"
            ],
            "name": "Linux",
            "defines": [
                "STM32WB",
                "STM32WBxx",
                "STM32WB5x",
                "DMESG_SERIAL_DEBUG",
                "USE_HAL_DRIVER",
                "DEVICE_SRAM_BASE=0x20000000",
                "DEVICE_SRAM_END=0x20018000",
                "DEVICE_STACK_BASE=DEVICE_SRAM_END"
            ],
            "intelliSenseMode": "gcc-arm",
            "cStandard": "c11",
            "cppStandard": "c++17"
        }
    ],
    "version": 4
}
